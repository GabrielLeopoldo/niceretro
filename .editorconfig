# This file is for unifying the coding style for different editors and IDEs
# See editorconfig.org

root = true

[*]
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false
text_width = 80
line_width = 80
wrap_width = 80
max_line_size = 80
right_margin = 80
